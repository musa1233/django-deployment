# -*- coding: utf-8 -*-
import re
import sys

from celery.__main__ import _compat_worker

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw|\.exe)?$', '', sys.argv[0])
    sys.exit(_compat_worker())
